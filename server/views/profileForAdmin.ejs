                    formData.append("id_of_user", id_of_user.trim());
                    formData.append("status", status.trim());

                    // console.log(formData);

                    if (formData !== undefined && formData !== null && formData !== '') {
                        async function conselling_of_user_status_Func(formData) {
                            try {
                                const options = {
                                    method: 'POST',
                                    mode: "no-cors",
                                    body: formData,
                                };
                                await fetch("/api/conselling_of_user_status", options)
                                    .then((response) => response.json())
                                    .then((obj) => obj)
                            } catch (error) {
                                console.log("This is the error in profileForMember.ejs > answer_is_yes_for_user_btns[i] > conselling_of_user_status_Func", error)
                            }
                        }
                        conselling_of_user_status_Func(formData);
                    }
                });
            }
        }, 500);

        // Getting or setting verification of Colleges 
        setTimeout(() => {
            const all_College_Info = document.querySelectorAll(".College_Info");
            const verification_process_of_college = document.querySelectorAll('.verification_process_of_college');
            const all_id_of_college = document.querySelectorAll('.id_of_college');
            const all_verification_processStoredHere = document.querySelectorAll(".verification_processStoredHere");
            const all_false_verification_process = document.querySelectorAll('.false_verification_process');
            const true_verification_process = document.querySelectorAll(".true_verification_process");
            const all_verification_processStoredFromSelectedOption = document.querySelectorAll(".verification_processStoredFromSelectedOption");
            // const all_value_Of_preference_FromDatabase = document.querySelectorAll(".value_Of_verification_process_FromDatabase");

            for (let i = 0; i < verification_process_of_college.length; i++) {

                let storedValue = all_verification_processStoredHere[i].innerHTML.trim();

                // if (storedValue === 'New') {
                if (storedValue === 'false') {
                    all_false_verification_process[i].innerHTML = storedValue;
                    true_verification_process[i].innerHTML = 'true';
                    // all_College_Info[i].style.backgroundColor = "#d1d2d1";
                } else if (storedValue === 'true') {
                    all_false_verification_process[i].innerHTML = 'false';
                    true_verification_process[i].innerHTML = storedValue;
                    // all_College_Info[i].style.backgroundColor = "Yellow";
                }


                verification_process_of_college[i].addEventListener("change", () => {
                    if (verification_process_of_college[i].value === "false") {
                        all_verification_processStoredHere[i].style.display = 'none';
                        all_verification_processStoredFromSelectedOption[i].style.display = 'flex';
                        all_verification_processStoredFromSelectedOption[i].innerHTML = 'false';
                        all_College_Info[i].style.backgroundColor = "#d1d2d1";

                    } else if (verification_process_of_college[i].value === "true") {
                        // console.log("all_verification_processStoredHere[i]: ", all_verification_processStoredHere[i].innerHTML.trim());
                        all_verification_processStoredHere[i].style.display = 'none';
                        all_verification_processStoredFromSelectedOption[i].style.display = 'flex';
                        all_verification_processStoredFromSelectedOption[i].innerHTML = 'true';
                        all_College_Info[i].style.backgroundColor = "Yellow";

                    }

                    const id_of_user = all_id_of_college[i].innerHTML.trim();
                    const verification = verification_process_of_college[i].value;

                    const formData = new FormData();
                    formData.append("id_of_user", id_of_user);
                    formData.append("verification", verification);

                    if (formData !== undefined || formData !== null || formData !== '') {
                        async function college_verification_Func(formData) {
                            try {
                                const options = {
                                    method: 'POST',
                                    mode: "no-cors",
                                    body: formData,
                                };
                                await fetch("/api/college_verification_process", options)
                                    .then((response) => response.json())
                                    .then((obj) => obj)
                            } catch (error) {
                                console.log("Error: ", error);
                            }
                        }
                        college_verification_Func(formData);
                    }
                });
            }
        }, 500);

        // <---------- Deleting a College process start ----------> 
        const delete_College_Btns = document.querySelectorAll('.delete_College_Btn');

        for (let i = 0; i < delete_College_Btns.length; i++) {
            const College_Delete_Askers = document.querySelectorAll('.College_Delete_Asker');
            delete_College_Btns[i].addEventListener("click", () => {

                if (College_Delete_Askers[i].style.display === 'flex') {
                    College_Delete_Askers[i].style.display = 'none';
                } else {
                    College_Delete_Askers[i].style.display = 'flex';
                }
            });

        }

        const answer_is_no_for_College_btns = document.querySelectorAll('.answer_is_no_for_College_btn');

        for (let i = 0; i < answer_is_no_for_College_btns.length; i++) {

            const College_Delete_Askers = document.querySelectorAll('.College_Delete_Asker');
            answer_is_no_for_College_btns[i].addEventListener("click", () => {

                if (College_Delete_Askers[i].style.display === 'none') {
                    College_Delete_Askers[i].style.display = 'flex';
                } else {
                    College_Delete_Askers[i].style.display = 'none';
                }
            });

        }

        const answer_is_yes_for_College_btns = document.querySelectorAll('.answer_is_yes_for_College_btn');

        for (let i = 0; i < answer_is_yes_for_College_btns.length; i++) {

            const College_Delete_Askers = document.querySelectorAll('.College_Delete_Asker');
            const College_Infos = document.querySelectorAll('.College_Info');
            const College_Ids = document.querySelectorAll('.College_Id');

            answer_is_yes_for_College_btns[i].addEventListener("click", () => {

                const College_Id = College_Ids[i].textContent;

                if (College_Delete_Askers[i].style.display === 'none') {
                    College_Delete_Askers[i].style.display = 'flex';
                } else {
                    College_Delete_Askers[i].style.display = 'none';
                }

                const formData = new FormData();
                formData.append("College_Id", College_Id.trim());

                console.log("College_Id: ", formData);

                if (formData !== undefined || formData !== null || formData !== '') {
                    async function delete_College_By_Admin_Func(formData) {
                        try {
                            const options = {
                                method: 'POST',
                                mode: "no-cors",
                                body: formData,
                            };
                            await fetch("/api/delete_College_By_Admin", options)
                                .then((response) => response.json())
                                .then((obj) => obj)
                        } catch (error) {
                            console.log("This is the error in profileForMember.ejs > answer_is_yes_for_user_btns[i] > delete_College_By_Admin", error)
                        }
                    }
                    delete_College_By_Admin_Func(formData);
                }

                College_Infos[i].remove();
            });

        }
        // <---------- Deleting a College process end ---------->
    </script>
