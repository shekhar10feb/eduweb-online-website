                                                <button type="submit"
                                                    class="bg-white mx-2 p-1 px-3 rounded-sm answer_is_no_for_user_btn">No</button>
                                            </div>
                                            <p class="hidden userId">
                                                <%= item._id %>
                                            </p>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <% }) %>


