        college_description_read_more_preview.style.display = 'none';
        college_description_read_less_preview.style.display = 'flex'
      } else {
        college_description_read_more_preview.style.display = 'flex'
        college_description_read_less_preview.style.display = 'none';
      }
    }

    const many_more_entrance_exam = document.querySelector('#many_more_entrance_exam');

    if (many_more_entrance_exam !== '' && many_more_entrance_exam !== undefined && many_more_entrance_exam !== null) {
      const entrance_exams_hidden = document.querySelector('.entrance_exams_hidden');
      many_more_entrance_exam.addEventListener("mouseover", function() {
        if (entrance_exams_hidden.style.display === 'flex') {
          entrance_exams_hidden.style.display = 'none';
          entrance_exams_hidden.style.position = "absolute";
        } else {
          entrance_exams_hidden.style.display = 'flex';
          entrance_exams_hidden.style.position = "absolute";
        }
        // }
      });

      many_more_entrance_exam.addEventListener("mouseout", function() {
        if (entrance_exams_hidden.style.display === 'none') {
          entrance_exams_hidden.style.display = 'flex';
          entrance_exams_hidden.style.position = "absolute";
        } else {
          entrance_exams_hidden.style.display = 'none';
          entrance_exams_hidden.style.position = "absolute";
        }
      });
    }

    const many_more_courses_offered = document.querySelector('#many_more_courses_offered');

    if (many_more_courses_offered !== '' && many_more_courses_offered !== undefined && many_more_courses_offered !== null) {
      const courses_offered_hidden = document.querySelector('.courses_offered_hidden');
      many_more_courses_offered.addEventListener("mouseover", function() {
        if (courses_offered_hidden.style.display === 'flex') {
          courses_offered_hidden.style.display = 'none';
          courses_offered_hidden.style.position = "absolute";
        } else {
          courses_offered_hidden.style.display = 'flex';
          courses_offered_hidden.style.position = "absolute";
        }
        // }
      });

      many_more_courses_offered.addEventListener("mouseout", function() {
        if (courses_offered_hidden.style.display === 'none') {
          courses_offered_hidden.style.display = 'flex';
          courses_offered_hidden.style.position = "absolute";
        } else {
          courses_offered_hidden.style.display = 'none';
          courses_offered_hidden.style.position = "absolute";
        }
      });
    }

    // User is giving rating to the particular College 
    const star_reviews_from_database = document.querySelectorAll('.star_review_from_database');
    const ratingValuesStoredHere = document.querySelectorAll('.ratingValueStoredHere');
    const college_ratings = document.querySelectorAll('.college_rating1');
    const id_of_colleges = document.querySelectorAll('.id_of_college');
    const temporary_display_dates = document.querySelectorAll('.temporary_display_date');

    for (let i = 0; i < star_reviews_from_database.length; i++) {
      if (college_ratings[i].value && id_of_colleges[i].textContent) {
        function user_giving_start_rating_to_college() {
          star_reviews_from_database[i].addEventListener('change', (e) => {
            e.preventDefault();

            // Display value to frontend 
            ratingValuesStoredHere[0].textContent = college_ratings[i].value + '/5';

            // Sending values to backend
            const college_rating_value = college_ratings[i].value;
            const college_id = id_of_colleges[i].textContent;

            const formData = new FormData();
            formData.append("star_rating", college_rating_value.trim());
            formData.append("college_id", college_id.trim());

            // console.log('formData: ', formData);

            if (formData) {
              let star_rating_given_by_user_to_college_Func = async (formData) => {
                try {
                  const options = {
                    method: 'POST',
                    mode: "no-cors",
                    body: formData,
                  };
                  let fetchItem = await fetch("/api/star_rating_given_by_user_to_college", options)
                    .then((response) => response.json())
                    .then(obj => {})
                    .then(console.log("received!"));
                } catch (error) {
                  console.log("This is the error in search_College.ejs > two_star > star_rating_given_by_user_to_college_Func", error)
                }
              }
              star_rating_given_by_user_to_college_Func(formData);
            }
          });
        }
        user_giving_start_rating_to_college();
      }
    }

    // User is giving review to the particular College 
    const form_for_comment_by_users = document.querySelectorAll('.form_for_comment_by_user');
    const usernames = document.querySelectorAll('.username_for_comment');
    const else_part_1s = document.querySelectorAll('.else_part_1');
    const users_from_database = document.querySelectorAll('.user_from_database');
    const college_comment_related_lengths = document.querySelectorAll('.college_comment_related_length');
    const if_college_comment_related_lengths_is_zero = document.querySelectorAll('.if_college_comment_related_length_is_zero');

    for (let i = 0; i < form_for_comment_by_users.length; i++) {
      if (form_for_comment_by_users[i] && id_of_colleges[i] && usernames[i]) {
        function user_giving_comment_to_college() {
          form_for_comment_by_users[i].addEventListener('submit', (e) => {
            e.preventDefault();

            const date = new Date();
            else_part_1s[0].children[2].children[0].children[0].textContent = date.toDateString();

            if (Number(college_comment_related_lengths[i].textContent) === 0) {
              if_college_comment_related_lengths_is_zero[i].style.display = 'none';
              users_from_database[0].style.display = 'none';
              else_part_1s[0].children[0].children[0].textContent = usernames[i].textContent;
              else_part_1s[0].children[0].children[1].children[0].textContent = ratingValuesStoredHere[0].textContent;
              else_part_1s[0].children[1].children[0].textContent = form_for_comment_by_users[i].children[0].value;
              else_part_1s[0].style.display = 'flex';
            }

            // Display value to frontend 
            if ((college_comment_related_lengths[i].textContent) > 0) {
              if_college_comment_related_lengths_is_zero[i].style.display = 'none';
              users_from_database[0].style.display = 'none';
              else_part_1s[0].children[0].children[0].textContent = usernames[i].textContent;
              else_part_1s[0].children[0].children[1].children[0].textContent = ratingValuesStoredHere[0].textContent;
              else_part_1s[0].children[1].children[0].textContent = form_for_comment_by_users[i].children[0].value;
              else_part_1s[0].style.display = 'flex';
            }

            // Sending values to backend
            const comment_by_user = form_for_comment_by_users[i].children[0].value
            const username = usernames[i].textContent;
            const college_rating = ratingValuesStoredHere[0].textContent;
            const collegeId = id_of_colleges[i].textContent;

            const formData = new FormData();
            formData.append("comment_by_user", comment_by_user.trim());
            formData.append("username_for_comment", username.trim());
            formData.append("college_rating", college_rating.trim());
            formData.append("collegeId", collegeId.trim());

            if (formData) {
              let comment_user_to_college_Func = async () => {
                try {
                  const options = {
                    method: 'POST',
                    mode: "no-cors",
                    body: formData,
                  };
                  let fetchItem = await fetch("/api/comment_user_to_college", options)
                } catch (error) {
                  console.log("This is the error in search_College.ejs > two_star > star_rating_given_by_user_to_college_Func", error)
                }
              }
              comment_user_to_college_Func();
            }
            form_for_comment_by_users[i].children[0].value = '';
          })
        }
        user_giving_comment_to_college();
      }
    }

    // User wants to delete his own comment 
    const delete_comments_by_user = document.querySelectorAll('.delete_comment_by_user');

    for (let i = 0; i < delete_comments_by_user.length; i++) {
      if (usernames[0].textContent && id_of_colleges[0].textContent) {
        delete_comments_by_user[i].addEventListener('click', () => {

          else_part_1s[0].remove();
          users_from_database[0].remove();

          if (Number(college_comment_related_lengths[0].textContent) === 0) {
            if_college_comment_related_lengths_is_zero[i].style.display = 'flex';
          }

          const username = usernames[0].textContent;
          const collegeId = id_of_colleges[0].textContent;

          const formData = new FormData();
          formData.append("username_for_comment", username.trim());
          formData.append("collegeId", collegeId.trim());

          if (formData) {
            let user_wants_to_delete_his_comment_Func = async () => {
              try {
                const options = {
                  method: 'POST',
                  mode: "no-cors",
                  body: formData,
                };
                let fetchItem = await fetch("/api/user_wants_to_delete_his_comment ", options)
              } catch (error) {
                console.log("This is the error in search_College.ejs > two_star > star_rating_given_by_user_to_college_Func", error)
              }
            }
            user_wants_to_delete_his_comment_Func();
          }
        });
      }
    }
  </script>

