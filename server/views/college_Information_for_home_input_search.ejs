        document.querySelector('.dropDownDiv').style.display = 'flex';
      }
    }

    function myFunctionMobileScreen() {
      if (document.querySelector('.dropDownDivMobileScreen').style.display === 'flex') {
        document.querySelector('.dropDownDivMobileScreen').style.display = 'none';
      } else {
        document.querySelector('.dropDownDivMobileScreen').style.display = 'flex';
      }
    }

    function editFunc() {
      if (document.querySelector('.editBoard').style.display === 'flex') {
        document.querySelector('.editBoard').style.display = 'none'
        document.querySelector('.previewBoard').style.display = 'flex';
      } else {
        document.querySelector('.editBoard').style.display = 'flex'
        document.querySelector('.previewBoard').style.display = 'none';
      }
    }

    function previewFunc() {
      if (document.querySelector('.previewBoard').style.display === 'flex') {
        document.querySelector('.previewBoard').style.display = 'none';
        document.querySelector('.editBoard').style.display = 'flex'
      } else {
        document.querySelector('.editBoard').style.display = 'none'
        document.querySelector('.previewBoard').style.display = 'flex';
      }

    }

