<!-- Header -->
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>EduWeb | Discover Colleges, Courses and Exams for higher education in India</title>
  <link rel="stylesheet" href="/styles/style.css">
  <style>
    .topHeadPhoto {
      border-radius: 50% 50%;
    }

    .scaleImage {
      transition: transform .5s ease-in;
    }

    .scaleImage:hover {
      transform: scale(0.9);
    }

    .searchBar input {
      width: 72%;
    }

    .searchBar button {
      width: 22%;
    }

    .changeClass {
      background-color: green;
    }

    .infoPage {
      width: 75%;
    }

    .adPage {
      height: 80vh;
    }
  </style>
</head>

<body class="bg-gray-300">
  <header class="w-full flex justify-center items-center text-white fixed top-0 z-10" style="background-color: #1e40af;">

    <!-- Desktop Version -->
    <nav class="w-full hidden lg:flex justify-between items-center px-1 py-1">
      <div class="lg:w-36 lg:px-1">
        <a href="https://eduweb-online-website.onrender.com" rel="noopener noreferrer"><img src="../logoPre.png" class="w-full" alt=""></a>
      </div>
      <ul class="lg:w-3/6 h-full flex justify-evenly items-center flex-row text-sm lg:font-bold">
        <li class="w-auto h-full flex justify-between items-center flex-row select-none cursor-pointer lg:p-1 xl:mr-4 lg:mr-2 lg:rounded-sm text-white hover:bg-white hover:text-blue-800">
          <a href="/api/home" class="w-full flex justify-center items-center no-underline px-4" rel="noopener noreferrer">
            <div class="w-full flex justify-center items-center flex-row">
              <p>Home</p>
            </div>
          </a>
        </li>
        <li class="w-auto h-full flex justify-between items-center flex-row select-none cursor-pointer lg:p-1 xl:mr-4 lg:mr-2 lg:rounded-sm text-white hover:bg-white hover:text-blue-800">
          <a href="/api/profile" class="w-full flex justify-center items-center no-underline px-4" rel="noopener noreferrer">
            <div class="w-full flex justify-center items-center flex-row">
              <p>Profile</p>
            </div>
          </a>
        </li>
        <li class="w-auto h-full flex justify-between items-center flex-row select-none cursor-pointer lg:p-1 xl:mr-4 lg:mr-2 lg:rounded-sm text-white hover:bg-white hover:text-blue-800">
          <a href="/api/blogs" class="w-full flex justify-center items-center no-underline px-4" rel="noopener noreferrer">
            <div class="w-full flex justify-center items-center flex-row">
              <p>Blogs</p>
            </div>
          </a>
        </li>
        <li class="w-auto h-full flex justify-between items-center flex-row select-none cursor-pointer lg:p-1 xl:mr-4 lg:mr-2 lg:rounded-sm text-white hover:bg-white hover:text-blue-800">
          <a href="/api/settings" class="w-full flex justify-center items-center no-underline px-4" rel="noopener noreferrer">
            <div class="w-full flex justify-center items-center flex-row">
              <p>Settings</p>
            </div>
          </a>
        </li>
        <li class="w-auto h-full flex justify-between items-center flex-row select-none cursor-pointer lg:p-1 xl:mr-4 lg:mr-2 lg:rounded-sm text-white hover:bg-white hover:text-blue-800">
          <a href="/api/logout" class="w-full flex justify-center items-center no-underline px-4" rel="noopener noreferrer">
            <div class="w-full flex justify-center items-center flex-row">
              <p>Logout</p>
            </div>
          </a>
        </li>
      </ul>

      <div class="lg:w-3/12 w-full flex justify-end items-center text-white select-none">
        <div class="w-auto flex justify-center items-center flex-row text-sm lg:font-bold px-2">
          <p class="w-auto">@<%= user.username ? (user.username.length > 7 ? user.username.slice(0, 7) + '...' : user.username.slice(0)) : '' %></p>
        </div>
        <div class="w-20 flex justify-around items-center">
          <img src="<%= user.profile_image %>" class="w-10 object-fill aspect-square topHeadPhoto" alt="">
        </div>
      </div>
    </nav>

    <!-- Mobile Version -->
    <nav class="w-full lg:hidden flex justify-between items-center px-1 py-1 z-10">
      <div class="w-1/2 flex justify-start items-center flex-row">
        <div class="md:w-24 w-20 rounded-sm">
          <a href="https://eduweb-online-website.onrender.com" class="cursor-pointer" rel="noopener noreferrer"><img src="../logoPre.png" class="w-32" alt=""></a>
        </div>
      </div>
      <div class="w-full flex justify-end items-center cursor-pointer" onclick="myFunctionMobileScreen()">
        <div class="w-auto flex justify-center items-center flex-row md:text-sm text-xs md:font-semibold sm:font-medium font-normal px-2">
          <p class="w-auto">@<%= user.username ? (user.username.length > 7 ? user.username.slice(0, 7) + '...' : user.username.slice(0)) : '' %></p>
        </div>
        <img src="<%= user.profile_image %>" class="md:w-10 w-8 object-fll aspect-square topHeadPhoto" alt="">
        <img src="../sortDown.png" class="md:w-3 w-2 md:mx-2 mx-1" alt="">
      </div>
      <div class="w-auto dropDownDivMobileScreen hidden justify-center items-center flex-col text-sm text-black absolute md:top-8 top-6 right-0 bg-white border border-white mt-2 rounded-sm z-1 overflow-auto">
        <a href="/api/home" class="w-full flex justify-center items-center no-underline hover:bg-blue-300 md:p-2 p-1 md:px-4 px-2" rel="noopener noreferrer">
          <div class="w-full flex justify-center items-center flex-row">
            <img src="../home.png" class="md:w-5 w-3" alt="">
            <p class="md:text-sm text-xs">Home</p>
          </div>
        </a>
        <a href="/api/profile" class="w-full flex justify-center items-center no-underline hover:bg-blue-300 md:p-2 p-1 md:px-4 px-2" rel="noopener noreferrer">
          <div class="w-full flex justify-center items-center flex-row">
            <img src="../personProfile.png" class="md:w-5 w-3" alt="">
            <p class="md:text-sm text-xs">Profile</p>
          </div>
        </a>
        <a href="/api/blogs" class="w-full flex justify-start items-center no-underline hover:bg-blue-300 md:p-2 p-1 md:px-4 px-2" rel="noopener noreferrer">
          <div class="w-full flex justify-start items-center flex-row">
            <img src="./blog.png" class="md:w-5 w-3" alt="">
            <p class="md:text-sm text-xs">Blogs</p>
          </div>
        </a>
        <a href="/api/settings" class="w-full flex justify-center items-center no-underline hover:bg-blue-300 md:p-2 p-1 md:px-4 px-2" rel="noopener noreferrer">
          <div class="w-full flex justify-center items-center flex-row">
            <img src="../settings.png" class="md:w-5 w-3" alt="">
            <p class="md:text-sm text-xs">Settings</p>
          </div>
        </a>
        <a href="/api/logout" class="w-full flex justify-center items-center no-underline hover:bg-blue-300 md:p-2 p-1 md:px-4 px-2" rel="noopener noreferrer">
          <div class="w-full flex justify-center items-center flex-row">
            <img src="../logout.png" class="md:w-5 w-3" alt="">
            <p class="md:text-sm text-xs">Logout</p>
          </div>
        </a>
      </div>
    </nav>
  </header>

  <!-- Main -->
  <main class="w-full flex justify-center items-center flex-row overflow-hidden lg:p-1 lg:mt-12 mt-8">
    <div class="w-full flex justify-center items-center flex-col">
      <div class="w-full flex justify-center items-center flex-col">
        <% advertisement.map((item, index)=> { %>
        <% if(item.image_resolution_value==='768px' || item.image_resolution_value==='640px' || item.image_resolution_value==='300px' ){ %>
        <!-- image_resolution_value === '768px' -->
        <% if(item.image_resolution_value==='768px' ){ %>
        <div class="w-full lg:hidden md:flex hidden h-full justify-center items-center flex-col">
          <span class="w-full h-full flex justify-center items-center">
            <img src="<%= item.filename %>" class="min-w-full min-h-full object-fill
                         aspect-auto advertisementPhoto" alt="" />
          </span>
        </div>
        <% } %>

        <!-- image_resolution_value === '640px' -->
        <% if(item.image_resolution_value==='640px' ){ %>
        <div class="w-full md:hidden sm:flex hidden h-full justify-center items-center flex-col">
          <span class="w-full h-full flex justify-center items-center">
            <img src="<%= item.filename %>" class="min-w-full min-h-full object-fill
                         aspect-auto advertisementPhoto" alt="" />
          </span>
        </div>
        <% } %>

        <!-- image_resolution_value === '300px' -->
        <% if(item.image_resolution_value==='300px' ){ %>
        <div class="w-full sm:hidden flex h-full justify-center items-center flex-col">
          <span class="w-full h-full flex justify-center items-center">
            <img src="<%= item.filename %>" class="min-w-full min-h-full object-fill
                         aspect-auto advertisementPhoto" alt="" />
          </span>
        </div>
        <% } %>
        <% } %>
        <% }) %>
      </div>

      <div class="w-full lg:hidden flex justify-center items-center text-sm bg-white font-bold sm:p-2 p-1 px-1 cursor-pointer">
        <a href="/api/previous_years_question_papers" class="w-full flex justify-center items-center bg-blue-800 rounded hover:bg-blue-700 text-white sm:p-2 p-1 px-1">
          <h1>Previous Years Question Papers</h1>
        </a>
      </div>

      <% college.map((item, index)=> { %>
      <% if (item.verification_process==='true' ) { %>
      <div class="w-full flex justify-center items-start flex-col relative">
        <div class="w-full flex justify-center items-start flex-row">
          <div class="lg:w-4/5 w-full h-full flex justify-center items-center flex-col">
            <div class="w-full flex justify-center items-center bg-white p-1 border border-gray-200">
              <div class="w-full md:flex hidden justify-start items-center text-xl font-bold mx-4 my-2">
                <h1 class="w-full lg:text-lg md:text-base underline text-wrap">
                  College: <%= item.name %>
                </h1>
              </div>
              <div class="w-full md:hidden flex justify-center items-center text-xl font-bold sm:m-2 mx-1 my-1">
                <h1 class="w-full text-center sm:text-sm text-xs underline text-wrap">
                  College: <%= item.name %>
                </h1>
              </div>
            </div>
            <div class="w-full flex justify-center items-start flex-col bg-transparent p-1">
              <p class="hidden id_of_college">
                <%= item._id %>
              </p>
              <div class="w-full flex justify-center items-center flex-col lg:bg-yellow-300 md:rounded-lg sm:rounded-sm">
                <% if(item.cover_image==='' || item.cover_image===undefined ||
                                        item.cover_image===null) { %>
                <img class="w-full hidden" src="" alt="" />

                <div class="w-full flex justify-center md:items-start items-center flex-row relative lg:top-2 bottom-3 md:mt-3 mt-4">
                  <!-- item Name -->
                  <div class="w-11/12 h-full flex justify-start items-center text-center rounded-md">
                    <div class="md:w-20 sm:w-16 w-12 sm:flex hidden justify-center items-center md:p-1 bg-yellow-400 mr-5 rounded-md">
                      <img src="<%= item.profile_image %>" class="w-full bg-white object-contain aspect-square rounded-md" alt="" />
                    </div>

                    <!-- For DOM Purpose -->
                    <!-- <p class="hidden college_id"><%= item._id %></p> -->
                    <input type="text" class="hidden college_id" value="<%= item._id %>">

                    <h1 class="lg:text-xl md:text-base sm:text-sm sm:flex hidden font-bold">
                      <%= item.name %>
                      <% if(item.college_short_name !=='' && item.college_short_name !==undefined && item.college_short_name !==null) { %>
                      (<%= item.college_short_name %>)
                      <% } %>
                    </h1>

                    <h1 class="sm:hidden flex font-bold" style="font-size: 0.7rem;">
                      <%= item.name %>
                      <% if(item.college_short_name !=='' && item.college_short_name !==undefined && item.college_short_name !==null) { %>
                      (<%= item.college_short_name %>)
                      <% } %>
                    </h1>
                  </div>
                </div>
                <% } else { %>
                <img class="w-full flex lg:h-48 md:h-36 sm:h-24 h-20 justify-center items-center object-fill aspect-square" src="<%= item.cover_image %>" alt="" />

                <div class="w-full sm:flex hidden justify-center md:items-start items-center flex-row relative lg:bottom-10 bottom-3 md:mt-3 mt-2">
                  <!-- item Name -->
                  <div class="w-11/12 h-full flex sm:justify-start justify-between sm:items-center items-start text-center md:rounded-md rounded-sm">
                    <% if(item.profile_image !=='' && item.profile_image !==undefined && item.profile_image !==null) { %>
                    <div class="md:w-20 sm:w-16 w-12 flex justify-center items-center sm:p-1 p-0 bg-yellow-400 mr-5 md:rounded-md rounded-sm">
                      <img src="<%= item.profile_image %>" class="w-full bg-white object-contain aspect-square md:rounded-md rounded-sm" alt="" />
                    </div>
                    <% } %>

                    <h1 class="xl:text-2xl lg:text-xl md:text-base sm:text-sm sm:flex hidden font-bold">
                      <%= item.name %>
                      <% if(item.college_short_name !=='' && item.college_short_name !==undefined && item.college_short_name !==null) { %>
                      (<%= item.college_short_name %>)
                      <% } %>
                    </h1>

                    <h1 class="sm:hidden flex font-bold" style="font-size: 0.7rem;">
                      <%= item.name %>
                      <% if(item.college_short_name !=='' && item.college_short_name !==undefined && item.college_short_name !==null) { %>
                      (<%= item.college_short_name %>)
                      <% } %>
                    </h1>
                  </div>
                </div>
                <% } %>

                <div class="w-full flex justify-center items-center flex-col">

                  <div class="w-full flex justify-start items-start flex-col lg:mt-6 md:mt-2">
                    <!-- Latest News and Events -->
                    <% if(item.latest_news_and_events !=='' && item.latest_news_and_events !==undefined && item.latest_news_and_events !==null) { %>
                    <div class="w-full flex justify-start items-center flex-col md:mb-4 mb-2">
                      <h2 class="md:w-11/12 w-full sm:text-sm text-xs md:font-semibold font-medium text-left my-1">
                        Latest News and Events
                      </h2>
                      <div class="md:w-11/12 w-full md:flex hidden justify-start items-start flex-col text-black md:text-sm text-xs bg-white md:p-4 sm:mb-3 p-2 md:rounded-md sm:rounded-sm rounded latest_news_and_events" style="font-size: 0.8rem;">
                        <ul class="list-disc w-auto flex justify-start items-start flex-col pl-4">
                          <% item.latest_news_and_events.split('++ ').map(item => { %>
                          <li>
                            <%= item %></li>
                          <%  }) %>
                        </ul>
                      </div>

                      <div class="md:w-11/12 w-full md:hidden flex justify-start items-start flex-col text-black md:text-sm text-xs bg-white p-2 mb-2 md:rounded-md sm:rounded-sm rounded latest_news_and_events" style="font-size: 0.7rem;">

                        <ul class="list-disc w-auto flex justify-center items-start flex-col pl-2">
                          <% item.latest_news_and_events.split('++ ').map(item => { %>
                          <li>
                            <%= item %>
                          </li>
                          <%  }) %>
                        </ul>
                      </div>
                    </div>
                    <% } %>

                    <!-- College Rank -->
                    <% if((item.engineering_rank !== '' &&    item.engineering_rank !== undefined &&    item.engineering_rank !== null) || 
                                                              (item.medical_rank !== '' &&        item.medical_rank !== undefined &&        item.medical_rank !== null) || 
                                                                  (item.law_rank !== '' &&            item.law_rank !== undefined &&            item.law_rank !== null) || 
                                                           (item.management_rank !== '' &&     item.management_rank !== undefined &&     item.management_rank !== null) || 
                                                       (item.fashion_design_rank !== '' && item.fashion_design_rank !== undefined && item.fashion_design_rank !== null) || 
                                                            (item.pharmacy_rank !== ''  &&       item.pharmacy_rank !== undefined &&       item.pharmacy_rank !== null) ||
                                                             (item.overall_rank !== ''  &&        item.overall_rank !== undefined &&       item.overall_rank !== null)) { %>
                    <div class="w-full flex justify-start items-center flex-col md:mb-4 mb-2">
                      <h2 class="md:w-11/12 w-full sm:text-sm text-xs md:font-semibold font-medium text-left my-1">
                        College Rank
                      </h2>

                      <div class="md:w-11/12 w-full flex justify-start items-center md:flex-row flex-wrap bg-white text-red-700 gap-1 p-2 md:rounded-md sm:rounded-sm rounded">
                        <% if(item.engineering_rank !=='' && item.engineering_rank !==undefined && item.engineering_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Engineering</h1>
                          <h1>
                            #<%= item.engineering_rank %>
                          </h1>
                        </div>
                        <% } %>

                        <% if(item.medical_rank !=='' && item.medical_rank !==undefined && item.medical_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Medical</h1>
                          <h1>
                            #<%= item.medical_rank %>
                          </h1>

                        </div>
                        <% } %>

                        <% if(item.law_rank !=='' && item.law_rank !==undefined && item.law_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Law</h1>
                          <h1>
                            #<%= item.law_rank %>
                          </h1>

                        </div>
                        <% } %>

                        <% if(item.management_rank !=='' && item.management_rank !==undefined && item.management_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Management</h1>
                          <h1>
                            #<%= item.management_rank %>
                          </h1>

                        </div>
                        <% } %>

                        <% if(item.fashion_design_rank !=='' && item.fashion_design_rank !==undefined && item.fashion_design_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Fashion
                            Design
                          </h1>
                          <h1>
                            #<%= item.fashion_design_rank %>
                          </h1>

                        </div>
                        <% } %>

                        <% if(item.pharmacy_rank !=='' && item.pharmacy_rank !==undefined && item.pharmacy_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Pharmacy
                          </h1>
                          <h1>
                            #<%= item.pharmacy_rank %>
                          </h1>

                        </div>
                        <% } %>

                        <% if(item.overall_rank !=='' && item.overall_rank !==undefined && item.overall_rank !==null) { %>
                        <div class="w-28 flex justify-start items-center flex-col md:text-sm text-xs sm:rounded-sm rounded latest_news_and_events border border-red-700">
                          <h1>Overall</h1>
                          <h1>
                            #<%= item.overall_rank %>
                          </h1>

                        </div>
                        <% } %>


                      </div>
                    </div>
                    <% } %>


                    <!-- About College -->
                    <% if((item.institute_type !=='' && item.institute_type !==undefined && item.institute_type !==null) || 
                                                          (item.approvals !=='' && item.approvals !==undefined && item.approvals !==null) || 
                                                              (item.courses_offered !=='' && item.courses_offered !==undefined && item.courses_offered !==null) || 
                                                       (item.college_campus !=='' && item.college_campus !==undefined && item.college_campus !==null) || 
                                                   (item.ownership !=='' && item.ownership !==undefined && item.ownership !==null) || 
                                                         (item.university_name !==''  && item.university_name !==undefined && item.university_name !==null) || 
                                                         (item.college_admission !==''  && item.college_admission !==undefined && item.college_admission !==null) || 
                                                         (item.entrance_exam !==''  && item.entrance_exam !==undefined && item.entrance_exam !==null) || 
                                                         (item.tel !==''  && item.tel !==undefined && item.tel !==null) || 
                                                         (item.email !==''  && item.email !==undefined && item.email !==null) || 
                                                         (item.city !==''  && item.city !==undefined && item.city !==null) || 
                                                         (item.district_name !==''  && item.district_name !==undefined && item.district_name !==null) || 
                                                         (item.state_or_ut_name !==''  && item.state_or_ut_name !==undefined && item.state_or_ut_name !==null) || 
                                                         (item.college_website !==''  && item.college_website !==undefined && item.college_website !==null) || 
                                                         (item.college_rating !==''  && item.college_rating !==undefined && item.college_rating !==null) || 
                                                         (item.college_details_brochure !==''  && item.college_details_brochure !==undefined && item.college_details_brochure !==null)) { %>
                    <div class="w-full flex justify-center items-center flex-col md:text-base sm:text-sm text-xs md:mb-4 mb-2">
                      <h2 class="md:w-11/12 w-full sm:text-sm text-xs md:font-semibold font-medium text-left my-1">
                        About College
                      </h2>
                      <div class="md:w-11/12 w-full flex justify-start items-center flex-wrap bg-white md:p-4 p-2 md:rounded-md sm:rounded-sm rounded">

                        <!-- Institute-Type -->
                        <% if(item.institute_type !=='' && item.institute_type!==undefined && item.institute_type !==null) { %>
                        <h2 class="sm:w-1/2 w-full flex jutify-start items-center text-left text-black md:text-sm text-xs my-1 sm:px-1">
                          Institute-Type: <span class="text-blue-700 pl-1 institute_type_preview">

                            <%= item.institute_type %>

                          </span>
                        </h2>
                        <% } %>

                        <!-- Approvals -->
                        <% if(item.approvals !=='' && item.approvals !==undefined && item.approvals !==null) { %>
                        <h2 class="sm:w-1/2 w-full flex jutify-start items-center text-left text-black md:text-sm text-xs my-1 sm:px-1">
                          Approvals: <span class="text-blue-700 pl-1 courses_offered_preview">

                            <%= item.approvals %>

                          </span>
                        </h2>
                        <% } %>

                        <!-- Courses Offered -->
                        <% if(item.courses_offered !=='' && item.courses_offered !==undefined && item.courses_offered !==null) { %>
                        <h2 class="sm:w-1/2 w-full flex jutify-start items-center text-left text-black md:text-sm text-xs my-1 sm:px-1">
                          Courses:

                          <span class="w-auto flex flex-row text-blue-700 pl-1 courses_offered_preview">
                            <%= item.courses_offered.split(',', 2) %>

                            <% item.courses_offered.split(',').map((item,index)=> { %>
                            <% if(index===2) { %>
                            <span class="cursor-pointer" id="many_more_courses_offered">
                              <u class="pl-1"> + many more</u>
                            </span>
                            <% } %>
                            <% }) %>


                            <span class="w-40 hidden justify-center items-center flex-wrap md:mt-6 mt-5 sm:ml-14 mr-8 md:p-2 p-1 bg-gray-100 rounded cursor-pointer font-semibold courses_offered_hidden border border-gray-300" style="font-size: 0.6rem;">
                              <% item.courses_offered.split(',').map((item, index)=> { %>
                              <% if(index> 1) { %>
                              <h1 class="w-auto text-purple-700">
                                <%= item %>
                                ,
