    const college_description_read_less_preview = document.querySelector('.college_description_read_less_preview');

    function read_more_btn() {
      if (college_description_read_less_preview.style.display === 'none') {
        college_description_read_less_preview.style.display = 'flex';
        college_description_read_more_preview.style.display = 'none'
      } else {
        college_description_read_more_preview.style.display = 'flex';
        college_description_read_less_preview.style.display = 'none';
      }
    }

    function read_less_btn() {
